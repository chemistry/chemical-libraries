{
    "name": "@chemistry/chem-js-lib",
    "version": "2.2.0",
    "description": "Open Cheminformatics Libraries",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "private": true,
    "keywords": [
        "cheminformatics",
        "chemistry",
        "javascript"
    ],
    "engines": {
        "node": ">=6.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chemistry/chem-js-lib.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "postinstall": "lerna bootstrap",
        "clean": "lerna clean && rimraf dist/* coverage/*",
        "bootstrap": "lerna bootstrap",
        "build": "lerna run build",
        "updated": "lerna updated",
        "test": "jest",
        "tdd": "jest --watch",
        "lint": "tslint -c tslint.json -p tsconfig.json 'packages/**/src/**/*.ts'",
        "lintfix": "tslint -c tslint.json -p tsconfig.json 'packages/**/src/**/*.ts' --fix",
        "do-publish": "npm run clean && npm run bootstrap && npm run build && lerna publish",
        "codecov": "codecov -t c0c5b758-349c-49bd-8c63-58fdc724e474 < coverage/lcov.info"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "23.3.9",
        "awesome-typescript-loader": "5.2.1",
        "codecov": "3.1.0",
        "jest": "23.6.0",
        "lerna": "3.4.3",
        "rimraf": "2.6.2",
        "ts-jest": "23.10.4",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "typescript": "3.1.6",
        "webpack": "4.25.1",
        "webpack-cli": "3.1.2"
    }
}
